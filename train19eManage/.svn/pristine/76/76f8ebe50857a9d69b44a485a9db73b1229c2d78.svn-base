@charset "utf-8";
/* CSS Document */
html{background-color:#fff;}
body,ul,li,p,h2,h3,h4,h5,dl,dt,dd,table,tr,td,select,option,textarea,span,button{margin:0;padding:0;font:12px/150% Arial,Verdana,"\5b8b\4f53";color:#333;}
input{padding:0 3px;}
ul{list-style:none;}
a{text-decoration:none;}
.oz{overflow:hidden;zoom:1;}
.fl{float:left;}
.fr{float:right;}
img{border:0;}
.spacer{content:".";height:0;clear:both;visibility:hidden;font-size:0;line-height:0;}
*:focus{outline:none;}
.btn{cursor:pointer;border:0;}
.wrap{width:960px;margin:10px auto 20px;}
textarea { resize: none; }
input::-moz-focus-inner {
border:none;
padding:0;
}
/*左侧公共部分 start*/
.side{width:120px;margin:0 auto;}
.side h2{height:40px;font:bold 14px/40px "Microsoft yahei";color:#fff;background-color:#2c99ff;padding-left:10px;}
.side-nav .parent-nav{border:1px solid #dadada;border-top:none;padding:0 10px;}
.side-nav span{margin-left:0px;color:#178eff;font:bold 14px/33px "Microsoft yahei";background:url(../images/sprite-icon.png) no-repeat;padding-left:10px;}
.side-nav .span1{background-position:0 3px;}
.side-nav .span2{background-position:0 -40px;}
.side-nav .span3{background-position:0 -82px ;}
.side-nav .parent-li ul{padding:4px 0;}
.side-nav .parent-nav a:link,.side-nav .parent-nav a:visited{line-height:22px;color:#555;padding-left:6px;}
.side-nav .parent-nav a:hover,.side-nav .parent-nav a:active{color:#f60;}
.side-nav .current a:link,.side-nav .current a:visited{color:#f60;background-position:126px -114px;}
.side-nav .current a:hover,.side-nav .current a:active{color:#f60;}
.side-nav li ul{border:1px solid #dedede;border-width:1px 0;}
.side-nav .last-li ul{border-bottom:none;}
/*左侧公共部分 end*/

