<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="repay">
	<select id="queryNeedRepay" resultClass="java.lang.String">
		SELECT hc.user_id FROM hc_userinfo AS hc
		 WHERE  hc.end_time <![CDATA[<]]> NOW() 
		 AND hc.user_level IN (0,1)
		 AND hc.estate='33'
		 LIMIT 0,5
	</select>
	
	<update id="updateNeedRepay" parameterClass="java.lang.String">
		UPDATE hc_userinfo AS hc SET hc.estate='44' WHERE hc.user_id =#user_id# AND hc.estate='33'
	</update>
</sqlMap>